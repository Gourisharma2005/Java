package EncapsulationDemo;

public class Student {

	private String name;
	private int age;
	private int marks;
	
	public String getName() {
		return name;
	}
	public int getAge() {
		return age;
	}
	public int getMarks() {
		return marks;
	}
	public void setName(String nm) {
		this.name = nm;
	}
	public void setAge(int a) {
		this.age = a;
	}
	public void setMarks(int mrk) {
		this.marks = mrk;
	}
}
